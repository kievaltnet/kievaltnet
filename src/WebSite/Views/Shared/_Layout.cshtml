<script type="text/javascript">
    $(function () { // restoring spam-protected emails

        var restoreSpamProtectedEmail = function (spamProtectedEmail) {
            var emailReversedBack = spamProtectedEmail.split('').reverse().join('');
            var emailWithRestoredAtSymbol = emailReversedBack.replace('#', '@@');
            return emailWithRestoredAtSymbol;
        }

        var isEmailSpamProtected = function (email) {
            return email.indexOf('#') > -1;
        }

        $('a.email').each(function () {
            var email = $(this).attr('href').replace('mailto:', '');

            if (isEmailSpamProtected(email)) {
                $(this).attr('href', 'mailto:' + restoreSpamProtectedEmail(email));
            }
        });
    });
